HDMI_S_FUNC_AUDIO_PLAYBACK_1920x1080_192KHz  source 'audio_display_functions.sh' ; devnode=`get_hdmi_audio_devnode`; modes=( $(get_video_connector_info hdmi 1920x1080-) ); do_cmd 'disp_audio_test hdmi modes 10 192000 $devnode'

