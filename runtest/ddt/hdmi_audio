# @name HDMI with Audio . 
# @desc Test cases corresponding to HDMI with Audio playback cases. 
# @requires hdmi/audio/playback
# @setup_requires hdmi

HDMI_S_FUNC_AUDIO_PLAYBACK_1920x1080_32KHz  source 'audio_display_functions.sh' ; devnode=`get_hdmi_audio_devnode`; modes=( $(get_video_connector_info hdmi 1920x1080-) ); do_cmd 'disp_audio_test hdmi modes 10 32000 $devnode'
HDMI_S_FUNC_AUDIO_PLAYBACK_1920x1080_44KHz  source 'audio_display_functions.sh' ; devnode=`get_hdmi_audio_devnode`; modes=( $(get_video_connector_info hdmi 1920x1080-) ); do_cmd 'disp_audio_test hdmi modes 10 44100 $devnode'
HDMI_S_FUNC_AUDIO_PLAYBACK_1920x1080_48KHz  source 'audio_display_functions.sh' ; devnode=`get_hdmi_audio_devnode`; modes=( $(get_video_connector_info hdmi 1920x1080-) ); do_cmd 'disp_audio_test hdmi modes 10 48000 $devnode'
