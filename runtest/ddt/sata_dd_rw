# @name SATA w/r test using existing FS
# @desc SATA device write/read test with with whatever fs in drive
# @requires sata 
# @setup_requires sata

SATA_S_FUNC_DD_RW_10M source "common.sh"; install_modules.sh "sata"; do_cmd blk_device_dd_readwrite_test.sh -b "1M" -c "10" -d "sata"

SATA_M_FUNC_DD_RW_1G source "common.sh"; install_modules.sh "sata"; do_cmd blk_device_dd_readwrite_test.sh -b "10M" -c "100" -d "sata"
