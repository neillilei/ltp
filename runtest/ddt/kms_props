# @name KMS properties test 
# @desc Test case to validate KMS object properties (alpha blending, 
# zorder, etc). 
# @requires kms

KMS_XS_FUNC_ZORDER source common.sh; /etc/init.d/weston stop && sleep 5; zorder.py 2>&1 | grep -i -e fail -e error -e invalid  -e 'not found' -e 'no such' && die "Zorder test failed"; echo "Test Passed"
KMS_XS_FUNC_CROPPING source common.sh; /etc/init.d/weston stop && sleep 5; cropping.py  2>&1 | grep -i -e fail -e error -e invalid  -e 'not found' -e 'no such' && die "Cropping test failed"; echo "Test Passed"
KMS_XS_FUNC_SRC_TRANSPARENCY source common.sh; /etc/init.d/weston stop && sleep 5; transparency.py ${MACHINE} src  2>&1 | grep -i -e fail -e error -e invalid -e 'not found' -e 'no such' && die "Src transparency test failed"; echo "Test Passed"
KMS_XS_FUNC_DST_TRANSPARENCY source common.sh; /etc/init.d/weston stop && sleep 5; transparency.py ${MACHINE} dst  2>&1 | grep -i -e fail -e error -e invalid -e 'not found' -e 'no such' && die "Dst transparency test failed"; echo "Test Passed"
KMS_XS_FUNC_ALPHA_SRC_TRANSPARENCY source common.sh; /etc/init.d/weston stop && sleep 5; transparency.py ${MACHINE} alpha_src  2>&1 | grep -i -e fail -e error -e invalid -e 'not found' -e 'no such' && die "Aplha src transparency test failed"; echo "Test Passed"
KMS_XS_FUNC_ALPHABLEND source common.sh; /etc/init.d/weston stop && sleep 5; alpha-blending.py  2>&1 | grep -i -e fail -e error -e invalid  -e 'not found' -e 'no such' && die "Alpha blending test failed"; echo "Test Passed"
KMS_XS_FUNC_GAMMA source common.sh; /etc/init.d/weston stop && sleep 5; gamma.py  2>&1 | grep -i -e fail -e error -e invalid -e 'not found' -e 'no such' && die "Gamma correction test failed"; echo "Test Passed"
KMS_XS_FUNC_GLOBAL_ALPHABLEND source common.sh; /etc/init.d/weston stop && sleep 5; global-alpha-blending.py  2>&1 | grep -i -e fail -e error -e invalid  -e 'not found' -e 'no such' && die "Alpha blending test failed"; echo "Test Passed"
KMS_XS_FUNC_DMABUF source common.sh; /etc/init.d/weston stop && sleep 5; dmabuf.py ${MACHINE} 2>&1 | grep -i -e fail -e error -e invalid -e 'not found' -e 'no such' && die "Aplha src transparency test failed"; echo "Test Passed"

