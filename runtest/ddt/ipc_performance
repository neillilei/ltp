# @name IPC  performance test script.
# @desc Test does IPC Performance test.

# @requires ipc 

IPC_S_PERF_IPC_PERF source "common.sh"; ipc_performance.sh -t 1000 -c 1;
IPC_S_FUNC_RPMSG_RPC source ipc_funcs.sh; rm_ipc_mods; setup_firmware '*-fw-rpmsg-rpc-test.*'; ins_ipc_mods rpc; num_proc=$(get_num_remote_procs); rpmsg_rpc_test $num_proc
IPC_S_FUNC_RPMSG_RPC_KILL source ipc_funcs.sh; rm_ipc_mods; setup_firmware '*-fw-rpmsg-rpc-test.*'; ins_ipc_mods rpc; num_proc=$(get_num_remote_procs); rpmsg_rpc_test $num_proc triple 5; rpmsg_rpc_test $num_proc
IPC_S_FUNC_RPMSG_RPC_MMU_FAULT source ipc_funcs.sh; rm_ipc_mods; setup_firmware '*-fw-rpmsg-rpc-test.*'; ins_ipc_mods rpc; num_proc=$(get_num_remote_procs); rpmsg_rpc_test $num_proc fault; rpmsg_rpc_test $num_proc






